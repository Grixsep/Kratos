{
  "module": "KratosMultiphysics.OptimizationApplication.algorithms",
  "type": "system_identification",
  "model_part_name": "Structure",
  "objective": {
    "response_name": "MeasurementLikelihoodResponseFunction",
    "type": "minimization",
    "scaling": 1.0
  },
  "controls": [
    {
      "model_part_names": ["Structure.all_nodes_elements_model_part"],
      "control_variable_name": "YOUNG_MODULUS",
      "mapping_options": {
        "use_constant_multiplication_factor": true,
        "constant_multiplication_factor": 10.0
      }
    }
  ],
  "echo_level": 0,
  "settings": {
    "echo_level": 0,
    "line_search": {
      "type": "const_step",
      "init_step": 0.1,
      "gradient_scaling": "inf_norm"
    },
    "conv_settings": {
      "type": "max_iter",
      "max_iter": 2
    }
  }
}
